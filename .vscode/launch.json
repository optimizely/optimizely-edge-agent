{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Wrangler",
            "type": "node",
            "request": "attach",
            "port": 9229,
            "cwd": "/",
            "resolveSourceMapLocations": null,
            "attachExistingChildren": false,
            "autoAttachChildProcesses": false,
            "sourceMaps": true
        },
        {
            "name": "Next.js: Vercel Example",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npm",
            "runtimeArgs": ["run", "dev"],
            "cwd": "${workspaceFolder}/examples/vercel",
            "console": "integratedTerminal",
            "skipFiles": ["<node_internals>/**"],
            "resolveSourceMapLocations": [
                "${workspaceFolder}/**",
                "!**/node_modules/**"
            ],
            "outFiles": [
                "${workspaceFolder}/examples/vercel/.next/**/*.js",
                "${workspaceFolder}/src/**/*.js"
            ],
            "env": {
                "NODE_OPTIONS": "--inspect"
            }
        }
    ]
}